@model IEnumerable<MatLanches.Models.Lanche>

    <div>
        <h2>Todos os Lanches</h2>
    </div>

<div class="container">div>
<div class="row">

@foreach(var lanche in Model)
{
    <div class="col-sm-4 col-md-4 col-lg-4">
        <h4>@lanche.Nome</h4>
        <p><img srvc="@lanche.ImagemUrl" class="img-fluid" /></p>
        <h3>Preço : @lanche.Preco.ToString("c")</h3>
        <h4>@lanche.Nome</h4>
        <p>@lanche.DescricaoCurta</p>
    </div>
}
</div>
</div>